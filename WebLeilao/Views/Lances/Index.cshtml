@model IEnumerable<TFLeilao.PL.Lance>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Vlr_lance)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Data)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Email)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Leilao)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Vlr_lance)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Data)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Email)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Leilao.LeilaoId)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.LanceId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.LanceId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.LanceId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
