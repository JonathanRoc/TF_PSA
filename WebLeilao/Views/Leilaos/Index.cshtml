@model IEnumerable<TFLeilao.PL.Leilao>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Valor)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Natureza)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Forma)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Data_ini)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Data_fin)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdUsuarioResponsavel)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Valor)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Natureza)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Forma)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Data_ini)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Data_fin)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdUsuarioResponsavel)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.LeilaoId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.LeilaoId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.LeilaoId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
